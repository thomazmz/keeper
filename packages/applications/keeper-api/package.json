{
  "name": "@keeper/api",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "exports": {
    "./domain": {
      "types": "./dist/domain/library.d.ts",
      "import": "./dist/domain/library.js",
      "require": "./dist/domain/library.js"
    },
    "./resources": {
      "types": "./dist/resources/library.d.ts",
      "import": "./dist/resources/library.js",
      "require": "./dist/resources/library.js"
    },
    "./routes": {
      "types": "./dist/routes/library.d.ts",
      "import": "./dist/routes/library.js",
      "require": "./dist/routes/library.js"
    }
  },
  "scripts": {
    "build": "tsc --project ./build.json",
    "start": "node --inspect --enable-source-maps --env-file=.env.local dist/server.js",
    "script": "node --inspect --enable-source-maps --env-file=.env.local dist/script.js"
  },
  "dependencies": {
    "@nodelith/core": "link:../nodelith/packages/core",
    "@nodelith/identity": "link:../nodelith/packages/identity",
    "@nodelith/injection": "link:../nodelith/packages/injection",
    "@nodelith/utilities": "link:../nodelith/packages/utilities",
    "acorn": "^8.15.0",
    "cors": "^2.8.6",
    "express": "^5.0.3",
    "googleapis": "^171.0.0",
    "jsonwebtoken": "^9.0.3",
    "mailparser": "^3.9.3",
    "mongodb": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mailparser": "^3.4.6",
    "@types/node": "^24.2.1",
    "typescript": "^5.9.2"
  }
}
